{
  "name": "quiz-backend",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "prettier": "prettier --cache --write .",
    "format": "pnpm run lint:fix && pnpm run prettier",
    "typecheck": "tsc",
    "wrangler:dev": "cd .. && wrangler pages dev client/dist",
    "dev": "pnpm run prisma:generate && pnpm run wrangler:dev",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "devDependencies": {
    "prisma": "^4.8.0"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "@trpc/server": "^10.6.0",
    "zod": "^3.20.2"
  }
}
